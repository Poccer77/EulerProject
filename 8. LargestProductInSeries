#reader(lib "htdp-advanced-reader.ss" "lang")((modname |7|) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f () #f)))
;;boy would you look at them parameters

(define (largest-subproduct length x)
  (local [(define number-length (string-length (number->string x)))
          (define (product it)
             (foldr * 1 (map (compose string->number string) (string->list (substring (number->string x) (- it length) it)))))
          (define (iterate current-largest it)
             (cond
               [(< (- it length) 0) current-largest]
               [else (iterate (if (> (product it) current-largest) (product it) current-largest) (- it 1))]))]
          (iterate 0 number-length))) 

(largest-subproduct 13 )